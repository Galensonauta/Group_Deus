{
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "script"
    },
    "extends": ["eslint:recommended", "prettier","plugin:node/recommended"],   
    "rules": {
      "no-console": "warn",
      "no-undef": "off",
    "no-unused-vars": "warn",
    "no-restricted-syntax": "off"
    },    
      "plugins": ["node"]
    }
    
  


 
  
 


